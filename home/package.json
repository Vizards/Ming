{
  "name": "home",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:qiankun": "UMI_ENV=qiankun umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/plugin-qiankun": "^2.25.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.4.25",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "umi": "^3.4.25",
    "yorkie": "^2.0.0"
  }
}
